<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Description>BepInEx support library for Il2Cpp games</Description>
        <TargetFramework>net9.0</TargetFramework>
        <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
        <RootNamespace>NextBepLoader.Core.IL2CPP</RootNamespace>
        <Configurations>Debug;Release;PreRelease</Configurations>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\NextBepLoader.Core\NextBepLoader.Core.csproj"/>
        <ProjectReference Include="..\NextBepLoader.Core.PreLoader\NextBepLoader.Core.PreLoader.csproj" PrivateAssets="All"/>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="HarmonyX" Version="2.14.0" />
        <PackageReference Include="Iced" Version="1.21.0"/>
        <PackageReference Include="Il2CppInterop.Generator" Version="1.4.6-ci.602" />
        <PackageReference Include="Il2CppInterop.HarmonySupport" Version="1.4.6-ci.602" />
        <PackageReference Include="Il2CppInterop.ReferenceLibs" Version="1.0.0" IncludeAssets="compile" PrivateAssets="all"/>
        <PackageReference Include="Il2CppInterop.Runtime" Version="1.4.6-ci.602" />
        <PackageReference Include="Microsoft.Extensions.Logging" Version="9.0.1" IncludeAssets="compile" PrivateAssets="all" />
        <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.1" IncludeAssets="compile" PrivateAssets="all" />
        <PackageReference Include="MonoMod.RuntimeDetour" Version="25.2.2" />
        <PackageReference Include="Samboy063.Cpp2IL.Core" Version="2022.1.0-pre-release.19" />
    </ItemGroup>

</Project>
